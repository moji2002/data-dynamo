{
  "name": "client",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "find:unused": "npx next-unused"
  },
  "sideEffects": false,
  "dependencies": {
    "clsx": "^1.2.1",
    "daisyui": "^2.22.0",
    "next": "latest",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "^18.0.15",
    "@types/react-dom": "18.0.5",
    "autoprefixer": "^10.4.7",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.2",
    "typescript": "4.7.2"
  },
  "next-unused": {
    "alias": {
      "@components/*": [
        "components/*"
      ],
      "@sections/*": [
        "sections/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@utils/*": [
        "utils/*"
      ]
    }
  },
  "include": [
    "components",
    "lib",
    "pages"
  ],
  "exclude": [],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
